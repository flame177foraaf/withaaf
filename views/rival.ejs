<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <%- include ('head') %>
    <link rel='stylesheet' href='/stylesheets/style.css' />

    <title>강적</title>
    <style media="screen">
      #left_sidenav {
        height: 100%;
        width: 15%;
        z-index: 1;
        overflow-x: hidden;
        position: absolute;
      }
      #left_sidenav ul{
        padding-left: 0px;
        font-size: 16px;
      }
      #left_sidenav a {
        padding: 6px 8px 6px 16px;
        text-decoration: none;
        color: black;
        display: block;
      }

      #left_sidenav a:hover {
        color: white;
        background-color: gray;
      }
      #left_sidenav li ul {
        background: ;
        display:none;  /* 평상시에는 서브메뉴가 안보이게 하기 */
        height:auto;
        padding:0px;
        margin:0px;
        border:0px;
        z-index:200;
        font-size: 12px;
      }
      #left_sidenav li:hover ul {
        font-size: 14px;
        padding-left: 10px;

        display:block;   /* 마우스 커서 올리면 서브메뉴 보이게 하기 */
      }
    </style>
  </head>
  <body style="margin:20px 50px 20px 50px">
    <div class="" style="width:300px" id="left_sidenav">
      <ul>
        <h3 class="" style="margin:5px" >강적</h3>
        <% for ( i = 0; i < list1.length; i++){ %>
          <% if (list1[i].rival_grade == '일반 강적'){ %>
            <% var rival_firstname = list1[i].rival_name.split(' "')[0] %>
            <li>
              <a href="/rival/<%- rival_firstname %>" class=" "> <%- list1[i].rival_name %></a>
            </li>
          <% } %>

        <% } %>


        <h3 class="" style="margin:5px" >희귀 강적</h3>
        <% for ( i = 0; i < list1.length; i++){ %>
          <% if (list1[i].rival_grade == '희귀 강적'){ %>
            <% var rival_firstname = list1[i].rival_name.split(' "')[0] %>
            <li>
              <a href="/rival/<%- rival_firstname %>" class=" "> <%- list1[i].rival_name %></a>
            </li>
          <% } %>
        <% } %>



          <h3 class="" style="margin:5px" >대강적</h3>
          <% for ( i = 0; i < list1.length; i++){ %>
            <% if (list1[i].rival_grade == '대강적'){ %>
              <% var rival_firstname = list1[i].rival_name.split(' "')[0] %>
              <li>
                <a href="/rival/<%- rival_firstname %>" class=" "> <%- list1[i].rival_name %></a>
              </li>

            <% } %>
          <% } %>
          <li>

          </li>
      </ul>
    </div>





    <div style="margin-left:350px">
      <% if (typeof(Search) != 'undefined'){ %>
        <table id="eqtable" style="width:100%;">


          <tbody>
            <th align="center" height="40" style="font-size:18px; font-weight:bold; color:#ffffff; background-color:#b8b6b6;" colspan="8">
             <%- list2[0].rival_name %>
            </th>
            <tr>
                <td style="padding:18px 18px 18 18px;" colspan="8">
                  <%- list2[0].rival_information %>
                </td>
            </tr>
          </tbody>

          </table>

          <table id="eqtable" style="width:100%;">

            <thead>
              <colgroup>
                <col width="10%">
                <col width="5%">
                <col width="5%">
                <col width="5%">
                <col width="10%">
                <col width="15%">
                <col width="20%">
                <col width="30%">
              </colgroup>
            </thead>
            <tbody>


            <th colspan="8">
                무기
            </th>

            <% if ( data1.length >= 1) { %>
              <% data1.forEach(function(item) { %>
                <% if (item.wpproperty.indexOf('/') != -1){ %>
                  <% var WpP = item.wpproperty.split('/') %>
                  <% if ( WpP[1] == ' 불 ') { %>
                    <tr class="" id="tabletr" style="white-space: pre-line; background-color:#FFD5DD">
                  <% } else if (WpP[1] == ' 물 ' ) { %>
                    <tr class="" id="tabletr" style="white-space: pre-line;  background-color:#DBE0FC">
                  <% } else if (WpP[1] == ' 바람 ') { %>
                    <tr class="" id="tabletr" style="white-space: pre-line;  background-color:#CEFFD3">
                  <% } else if (WpP[1] == ' 땅 ') { %>
                    <tr class="" id="tabletr" style="white-space: pre-line;  background-color:#F8EAD8">
                  <% } else if (WpP[1] == ' 정신 ') { %>
                    <tr class="" id="tabletr" style="white-space: pre-line;  background-color:#FFFFDB">
                  <% } else if (WpP[1] == ' 아스트랄 ') { %>
                    <tr class="" id="tabletr" style="white-space: pre-line;  background-color:#E4C1F2">
                  <% } else if (WpP[1] == ' 무속성 ') { %>
                    <tr class="" id="tabletr" style="white-space: pre-line;  background-color:#EAEAEA">
                  <% } %>
                <% } else { %>
                  <tr class="" id="tabletr" style="white-space: pre-line; ">
                <% } %>
                  <td><%- item.wpname %></td>
                  <td style='text-align: center'><%- item.wplimit %></td>
                  <td style='text-align: center'><%- item.wpsocket %></td>
                  <td style='text-align: center'><%- item.wpether %></td>
                  <td style='text-align: center'><%- item.wpstats %></td>
                  <td style='text-align: center'><%- item.wpproperty %></td>
                  <td><%- item.wpfeat %></td>
                  <td><%- item.wpcustom %></td>
                </tr>
              <% }) %>
            <% } else { %>
              <tr>
                <td colspan="8" style="text-align: center;"> - </td>
              </tr>
            <% } %>

              


              </tbody>
              </table>

              <table id="eqtable" style="width:100%;">

                <thead>
                  <colgroup>
                    <col width="10%">
                    <col width="5%">
                    <col width="5%">
                    <col width="5%">
                    <col width="10%">
                    <col width="15%">
                    <col width="20%">
                    <col width="30%">
                  </colgroup>
                </thead>
                <tbody>

                <th colspan="8">
                    방어구
                </th>

                <% if ( data2.length >= 1) { %>
                  <% data2.forEach(function(item) { %>
                    <tr>
                      <td><%- item.armname %></td>
                      <td style='text-align: center'><%- item.armlimit %></td>
                      <td style='text-align: center'><%- item.armsocket %></td>
                      <td style='text-align: center'><%- item.armether %></td>
                      <td style='text-align: center'><%- item.armstats %></td>
                      <td style='text-align: center'><%- item.armproperty %></td>
                      <td><%- item.armfeat %></td>
                      <td><%- item.armcustom %></td>
                    </tr>
                  <% }) %>
                <% } else { %>
                  <tr>
                    <td colspan="8" style="text-align: center;"> - </td>
                  </tr>
                <% } %>





                </tbody>
                </table>

                <table id="eqtable" style="width:100%;">

                  <thead>
                    <colgroup>
                      <col width="10%">
                      <col width="5%">
                      <col width="5%">
                      <col width="5%">
                      <col width="10%">
                      <col width="15%">
                      <col width="20%">
                      <col width="30%">
                    </colgroup>
                  </thead>
                  <tbody>

                <th colspan="8">
                    악세사리
                </th>

                <% if ( data3.length >= 1) { %>
                  <% data3.forEach(function(item) { %>
                    <tr>
                      <td><%- item.accname %></td>
                      <td style='text-align: center'><%- item.acclimit %></td>
                      <td style='text-align: center'><%- item.accsocket %></td>
                      <td style='text-align: center'><%- item.accether %></td>
                      <td style='text-align: center'><%- item.accstats %></td>
                      <td style='text-align: center'><%- item.accproperty %></td>
                      <td><%- item.accfeat %></td>
                      <td><%- item.acccustom %></td>
                    </tr>
                  <% }) %>
                <% } else { %>
                  <tr>
                    <td colspan="8" style="text-align: center;"> - </td>
                  </tr>
                <% } %>



              </tbody>

              </table>



              <table id="eqtable" style="width:100%;">

                <thead>
                  <colgroup>
                    <col width="30%">
                    <col width="35%">
                    <col width="35%">
                  </colgroup>
                </thead>
                <tbody>
                  <th colspan="3">
                      피트서포터
                  </th>
                  <% if ( data4.length >= 1) { %>
                    <% data4.forEach(function(item) { %>
                      <tr>
                        <td colspan=""><%- item.featname %></td>
                        <td colspan=""><%- item.feat %></td>
                        <td colspan=""><%- item.reversefeat %></td>
                      </tr>
                    <% }) %>
                  <% } else { %>
                    <tr>
                      <td colspan="3" style="text-align: center;"> - </td>
                    </tr>
                  <% } %>

                </tbody>

              </table>


                  <table id="eqtable" style="width:100%;">


                  <thead>
                    <colgroup>
                      <col width="20%">
                      <col width="20%">
                      <col width="20%">
                      <col width="40%">
                    </colgroup>
                  </thead>
                <tbody>
                  <th colspan="4">
                      루엘
                  </th>

                  <% if ( data5.length >= 1) { %>
                    <% data5.forEach(function(item) { %>
                      <tr>
                        <td colspan=""><%- item.gemgrade %></td>
                        <td colspan="" style='text-align: center'><%- item.gemname %></td>
                        <td colspan="" style='text-align: center'><%- item.gemobject %></td>
                        <td colspan=""><%- item.gemeffect %></td>
                      </tr>
                    <% }) %>
                  <% } else { %>
                    <tr>
                      <td colspan="4" style="text-align: center;"> - </td>
                    </tr>
                  <% } %>



                </tbody>

              </table>

          <table id="eqtable" style="width:100%;">

            <th>준비중</th>
            <tr>
              <td colspan="8" style="text-align:center">
                수집품 - 준비중입니다
              </td>
            </tr>
            <tr>
            </tr>
            <tr>
              <td colspan="8" style="text-align:center">
                조수 - 준비중입니다
              </td>
            </tr>
            <tr>
              <td colspan="8" style="text-align:center">
                칭호 - 준비중입니다
              </td>
            </tr>
          </tbody>



        </table>

      <% } %>


    </div>

  </body>
  <footer style="margin-left:350px">
    <%- include ('footer') %>
  </footer>
</html>
