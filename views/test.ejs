  //glow_tarray[idx].style.filter = "Glow(Color=" + glow_tfcolor[idx] + ", Strength=" + glow_i + ")";
  glow_tarray[idx].style.textShadow = repeatShadow("0px 0px " + (glow_i + 2) + "px " + glow_tfcolor[idx], glow_i + 3);
  glow_i++;
